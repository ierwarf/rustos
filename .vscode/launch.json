{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "QEMU (Release)",
            "type": "node-terminal",
            "request": "launch",
            "preLaunchTask": "Build OS",
            "command": "exec ./run.sh",
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "KVM (Release)",
            "type": "node-terminal",
            "request": "launch",
            "preLaunchTask": "Build OS",
            "command": "exec ./run.sh -enable-kvm",
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "QEMU x86 (Release)",
            "type": "node-terminal",
            "request": "launch",
            "preLaunchTask": "Build OS",
            "command": "exec ./run_x86.sh",
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "KVM x86 (Release)",
            "type": "node-terminal",
            "request": "launch",
            "preLaunchTask": "Build OS",
            "command": "exec ./run_x86.sh -enable-kvm",
            "cwd": "${workspaceFolder}"
        }
    ]
}